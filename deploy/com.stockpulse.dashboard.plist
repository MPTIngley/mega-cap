<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.stockpulse.dashboard</string>

    <key>ProgramArguments</key>
    <array>
        <string>/bin/zsh</string>
        <string>-c</string>
        <string>cd ~/Documents/AIGames/mega-cap &amp;&amp; source venv/bin/activate &amp;&amp; stockpulse dashboard</string>
    </array>

    <key>WorkingDirectory</key>
    <string>~/Documents/AIGames/mega-cap</string>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>~/Documents/AIGames/mega-cap/logs/dashboard.log</string>

    <key>StandardErrorPath</key>
    <string>~/Documents/AIGames/mega-cap/logs/dashboard.log</string>

    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/usr/local/bin:/usr/bin:/bin</string>
    </dict>
</dict>
</plist>
